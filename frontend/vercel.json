{
    "regions": ["hnd1"],
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/static/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "GENERATE_SOURCEMAP": "false"
    }
  }
  