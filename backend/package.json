{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:local": "cp .env.local .env && next dev -p 3002",
    "dev:cloud": "cp .env.development.cloud .env && next dev -p 3002",
    "dev:turbo": "next dev --turbo -p 3002",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start -p 3002",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf .next out",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "next": "15.3.3",
    "openai": "^5.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^4.10.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "typescript": "^5"
  }
}
